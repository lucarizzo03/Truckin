{
  "expo": {
    "name": "AutoPilot",
    "slug": "autopilot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.luca.autopilot",
      "supportsTablet": true
    },
    "android": {
      "package": "com.luca.autopilot",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "GOOGLE_API_KEY": "your-google-api-key-here"
    },
    "plugins": [
      "expo-dev-client"
    ]
  }
}